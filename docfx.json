{
  "metadata": [
    {
      "src": [
        {
          "files": ["PlayGroundApp/**/*.cs"],
          "exclude": ["**/obj/**", "**/bin/**"]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "includePrivateMembers": false
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"],
        "dest": "api"
      },
      {
        "files": ["README.md", "CODE_DOCUMENTATION.md"],
        "dest": "."
      },
      {
        "files": ["docs/**"],
        "dest": "docs"
      }
    ],
    "resource": [
      {
        "files": ["images/**"],
        "dest": "images"
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "sitemap": {
      "baseUrl": "https://github.com/iwanlenin/Playground",
      "priority": 1.0,
      "changefreq": "weekly"
    },
    "xrefService": [
      "https://xref.docs.microsoft.com/query?project=dotnet&uid={uid}"
    ]
  },
  "globalMetadata": {
    "_appTitle": "PlayGround App Documentation",
    "_appFooter": "© 2025 iwanlenin. All rights reserved.",
    "_author": "iwanlenin",
    "_description": "Complete documentation for PlayGround App - A .NET MAUI Receipt Management Application",
    "_gitContribute": {
      "branch": "main",
      "repository": "https://github.com/iwanlenin/Playground"
    }
  }
}
